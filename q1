num1 = int(input("Digite o primeiro número: "))
num2 = int1(input("Digite o segundo número: "))

if num1 == num2:
	
    	print("Os números são iguais.")
	
else:
    maior = max(num1, num2)
    menor = min(num1, num2)

    print("Os números são diferentes.")
    print("O maior número é:", maior)
    print("O menor número é:", menor)
